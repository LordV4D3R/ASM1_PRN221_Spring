<Window x:Class="ASM01Wpf.MainAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASM01Wpf"
        mc:Ignorable="d"
        Title="MainAdmin" Height="800" Width="1200">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Customer Manage" MinWidth="120" Click="CustomerManagebtn"/>
            <Button Content="Room Manage" MinWidth="120" Margin="10 0 0 0" Click="Room_Managebtn"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0 35 0 0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
